<Window x:Class="CameraArchery.View.CustomBrowserFolder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:self="clr-namespace:CameraArchery.View"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CustomBrowserFolder" Height="300" Width="300">
    <StackPanel>
        <TreeView Name="TreeControl">
            <TreeView.ItemTemplate>
                <HierarchicalDataTemplate DataType="{x:Type self:MenuItem}" ItemsSource="{Binding Items}">
                    <TextBlock Text="{Binding Title}" />                   
                </HierarchicalDataTemplate>
            </TreeView.ItemTemplate>
        </TreeView>

        <Button Content="add file"/>
        <Button Content="Return"/>
        <Button Content="OK"/>
    </StackPanel>
</Window>
